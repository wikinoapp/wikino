<% backlinks.each do |backlink| %>
  <%= render CardLinks::PageComponent.new(page: backlink.page, card_class: "h-full") %>
<% end %>

<% if pagination.has_next? %>
  <turbo-frame id="backlink-list">
    <%= render Buttons::LinkPaginationComponent.new(
      path: page_backlink_list_path(space.identifier, page.number, after: pagination.next_cursor)
    ) %>
  </turbo-frame>
<% end %>
