<% backlink_collection.backlinks.each do |backlink| %>
  <%= render Cards::PageCardComponent.new(page: backlink.page, class_name: "bg-base-300") %>
<% end %>

<% if backlink_collection.pagination.has_next %>
  <turbo-frame id="backlink-collection">
    <%= render Buttons::BacklinkPaginationButtonComponent.new(backlink_collection:) %>
  </turbo-frame>
<% end %>
