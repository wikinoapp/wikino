{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": false,
    "esModuleInterop": true,
    "importHelpers": true,
    "module": "CommonJS",
    "moduleResolution": "Node",
    "noEmit": true,
    "noImplicitThis": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "strict": false,
    "target": "ES5",
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules",
    "public/packs"
  ]
}
